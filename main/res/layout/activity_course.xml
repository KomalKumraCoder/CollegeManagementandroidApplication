<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.admin.collegemanagementsystem.Course">

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:text="Enter CourseId"
        android:ems="10"
        android:id="@+id/e1"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="52dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:text="Enter CouseName"
        android:ems="10"
        android:id="@+id/e2"
        android:layout_below="@+id/e1"
        android:layout_alignLeft="@+id/e1"
        android:layout_alignStart="@+id/e1" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:text="Enter CourseDuration"
        android:ems="10"
        android:id="@+id/e3"
        android:layout_below="@+id/e2"
        android:layout_alignRight="@+id/e2"
        android:layout_alignEnd="@+id/e2" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:text="Enter Course Fees"
        android:ems="10"
        android:id="@+id/e4"
        android:layout_below="@+id/e3"
        android:layout_alignLeft="@+id/e3"
        android:layout_alignStart="@+id/e3" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Insert"
        android:id="@+id/button2"
        android:layout_marginTop="70dp"
        android:layout_below="@+id/e4"
        android:layout_centerHorizontal="true"
        android:onClick="test" />
</RelativeLayout>
